{
  "auto.reload" : false,
  "environment.name" : "{{env-name}}",
  "graphite.enabled" : true,
  "graphite.host" : "{{graphite-host}}",
  "graphite.port" : 2003,
  "graphite.post.interval.seconds" : 60,
  "logging.consoleThreshold" : "off",
  "logging.fileThreshold" : "info",
  "logging.level" : "info",
  "logging.path" : "/var/log/{{app-name}}",
  "logging.stashThreshold" : "warn",
  "production" : {{is-prod}},
  "service.name" : "{{app-name}}",
  "service.port" : 8080,
  "shutdown.timeout.millis" : 5000,
  "start.timeout.seconds" : 120,
  "threads" : 254
}
